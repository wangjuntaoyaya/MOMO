<?xml version="1.0" encoding="utf-8"?>
<com.example.momo.widget.StickLayolut xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_dark"

    >
 <com.example.momo.widget.MessageTipView
     android:layout_width="match_parent"
     android:layout_height="300dp"
     android:id="@+id/message_tip_layout"



     >

  <ImageView
      android:layout_width="30dp"
      android:background="@drawable/ic_message_64dp"
      android:layout_height="30dp">

  </ImageView>

  <com.example.momo.widget.MessageView
      android:gravity="center"
        android:textSize="12sp"
      android:text="1"
      android:textColor="@color/white"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">

  </com.example.momo.widget.MessageView>

 </com.example.momo.widget.MessageTipView>






</com.example.momo.widget.StickLayolut>
